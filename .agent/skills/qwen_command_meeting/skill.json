{
  "name": "qwen_command_meeting",
  "description": "Meeting agent that uses qwen command line tool for interactive discussions",
  "version": "1.0.0",
  "author": "AGStock Team",
  "license": "MIT",
  "parameters": {
    "required": [],
    "optional": [
      {
        "name": "meeting_topic",
        "type": "string",
        "description": "Meeting topic or agenda",
        "default": "一般会議"
      },
      {
        "name": "duration_minutes",
        "type": "integer",
        "description": "Meeting duration in minutes",
        "default": 30
      },
      {
        "name": "language",
        "type": "string",
        "description": "Meeting language (ja, en, zh)",
        "default": "ja"
      },
      {
        "name": "qwen_model",
        "type": "string",
        "description": "Qwen model to use",
        "default": "qwen"
      },
      {
        "name": "save_transcript",
        "type": "boolean",
        "description": "Save meeting transcript",
        "default": true
      }
    ]
  },
  "dependencies": ["pydantic>=2.0.0", "python-dotenv>=1.0.0"],
  "entry_point": "scripts/qwen_command_meeting.py",
  "tags": ["ai", "meeting", "qwen", "cli", "discussion"],
  "category": "productivity"
}
