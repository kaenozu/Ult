{
  "name": "model_discussion",
  "description": "Compare and analyze responses from different AI models for code reviews, debugging, and technical discussions",
  "version": "1.0.0",
  "author": "AGStock Ult Team",
  "category": "ai-comparison",
  "tags": ["ai-models", "comparison", "discussion", "code-review", "analysis"],
  "capabilities": [
    "Multi-model response comparison",
    "Code review across different AI models",
    "Debugging approach comparison",
    "Technical discussion facilitation",
    "Performance analysis comparison",
    "Best practice identification",
    "Consensus building",
    "Bias detection in AI responses"
  ],
  "supported_file_types": [".py", ".ts", ".tsx", ".js", ".jsx", ".json", ".md"],
  "dependencies": {
    "python": ">=3.8",
    "openai": ">=1.3.0",
    "anthropic": ">=0.3.0",
    "requests": ">=2.31.0",
    "transformers": ">=4.36.0",
    "torch": ">=2.1.0",
    "ollama": ">=0.1.6",
    "huggingface-hub": ">=0.20.0",
    "groq": ">=0.9.0",
    "accelerate": ">=0.25.0",
    "bitsandbytes": ">=0.41.0"
  },
  "configuration": {
    "default_models": [
      {
        "name": "Big-Pickle",
        "provider": "huggingface",
        "model": "01-ai/Yi-1.5-34B-Chat",
        "api_key_required": false,
        "local_model": true,
        "specialty": "general"
      },
      {
        "name": "GLM-4.7",
        "provider": "huggingface",
        "model": "THUDM/glm-4-9b-chat-hf",
        "api_key_required": false,
        "local_model": true,
        "specialty": "general"
      },
      {
        "name": "MiniMax-M2.1",
        "provider": "huggingface",
        "model": "01-ai/Yi-1.5-34B-Chat",
        "api_key_required": false,
        "local_model": true,
        "specialty": "general"
      },
      {
        "name": "Grok-Code-Fast-1",
        "provider": "huggingface",
        "model": "xai-org/grok-1",
        "api_key_required": false,
        "local_model": true,
        "specialty": "code"
      }
    ],
    "max_tokens": 2000,
    "temperature": 0.7,
    "comparison_metrics": [
      "accuracy",
      "completeness",
      "clarity",
      "practicality",
      "innovation",
      "performance_impact"
    ],
    "output_format": "markdown",
    "save_discussions": true,
    "discussion_dir": ".agent/data/model_discussions"
  },
  "entry_point": "opencode_real_discussion.py",
  "documentation": "SKILL.md"
}
